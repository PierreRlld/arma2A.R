#' Title
#'
#' @param serie série Stationnaire d'entrée
#' @param p ordre partie autorégressive à tester
#' @param q ordre partie moyenne mobile à tester
#' @param lagmax lag max à tester pour les autocorrélations des résidus (statistique de Ljung-Box) > hypothèse forte résidus suivent un bruit blanc, test jusqu'à lag_max
#'
#' @return Synthese de TestAutocorr et TestSignificativite
#' @export
#'
#'#' @importFrom stats Box.test arima pnorm residuals
install.packages("~/arma2A_0.0.1.tar.gz", repos = NULL, type = "source")
require(fUnitRoots)
require(tseries)
require(aTSA)
require(astsa)
require(forecast)
require(BBmisc)
require(crayon)
require(ggplot2)
require(reshape2)
require(tidyr)
require(arma2A)
require(TSA)
require(lmtest)
require(arma2A)
data = read.csv(file = './git data/Ind_save.csv')
WT_ <- ts(drop_na(data['WT_balance']))
GDP_ <- ts(drop_na(data['WD_GDP']))
Imp_ <- ts(drop_na(data['WT_import']))
Exp_ <- ts(drop_na(data['WT_export']))
WT <- ts(WT_[1:length(WT_)])
GDP <- ts(GDP_[1:length(GDP_)])
Imp <- ts(WT_[1:length(Imp_)])
Exp <- ts(WT_[1:length(Exp_)])
plot(WT)
plot(GDP)
#Clairement pas l'air stationnaire
acf(GDP, lag.max = 40)
pacf(GDP,  lag.max = 40)
acf(WT, lag.max = 40)
pacf(WT,  lag.max = 40)
acf(Imp, lag.max = 40)
pacf(Imp,  lag.max = 40)
acf(Exp, lag.max = 40)
pacf(Exp,  lag.max = 40)
LWT <- diff(log(WT),1)
LGDP <- diff(log(GDP),1)
LImp <- diff(log(Imp),1)
LExp <- diff(log(Exp),1)
aLGDP <- LGDP-mean(LGDP)
